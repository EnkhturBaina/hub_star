{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@components/*": ["./src/components/*"],
      "@context/*": ["./src/context/*"],
      "@services/*": ["./src/services/*"],
      "@typeDefs/*": ["./src/types/*"],
      "@lib/*": ["./src/lib/*"],
      "@utils/*": ["./src/utils/*"],
      "@datas/*": ["./src/data/*"],
      "@styles/*": ["./src/styles/*"],
      "react": ["./node_modules/@types/react"]
    },
    "strictNullChecks": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/molecules/Profile/ListboxWrapper.jsx",
    "setupTests.js",
    "jest.config.js",
    "src/lib/**/js",
    "src/services/**/ts",
    "next.config.js"
  ],
  "exclude": ["node_modules"]
}
